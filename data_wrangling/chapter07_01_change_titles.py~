from csv import DictReader
import io
import sys
sys.stdout = io.TextIOWrapper(sys.stdout.buffer,encoding='utf8')

path = 'book/data/unicef/'
filename_data = 'mn.csv'
filename_header = 'mn_headers.csv'
data_rdr = DictReader(open(path + filename_data), 'rb')
header_rdr = DictReader(open(path + filename_header), 'rb')
header_row = [ head for row in header_rdr]
